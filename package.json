{
  "name": "next-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "yarn pretty && yarn lint --fix",
    "pretty": "prettier --check \"**/*.{json,md,js,mjs,ts,tsx,svg}\" --write",
    "test:e2e": "yarn playwright test",
    "generate:svg": "yarn svgr --ignore-existing -d src/assets/svg svg --no-dimensions && yarn format"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-query-devtools": "^4.29.5",
    "axios": "^1.3.6",
    "classnames": "^2.3.2",
    "i18next": "^22.4.15",
    "next": "13.3.1",
    "next-i18next": "^13.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.2.2",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@svgr/cli": "^7.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/eslint-plugin-query": "^4.29.4",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  }
}
